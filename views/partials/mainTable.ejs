<main class="bd-main col-lg-10">
    <div class="container-xxl">
        <div id="entry-count"><%= data.length %> Entries Found</div>
        <div>Price: $<%= dataFilters.x.min %> - $<%= dataFilters.x.max %> </div>
        <div>Dia: <%= dataFilters.d.min %>mm - <%= dataFilters.d.max %>mm</div>
        <div>Length: <%= dataFilters.l.min %>m - <%= dataFilters.l.max %>m</div>
        <table id="myTable" style="width: 100%"  class="table table-hover table-borderless table-sm">
            <thead>
                <tr>
                    <th></th>   <!-- Checkbox Column -->
                    <th>Name</th>
                    <th>Brand</th>
                    <th>Diameter (mm)</th>
                    <th>Length (m)</th>
                    <th>Price</th>
                </tr>
            </thead>
            <tbody>
                <% for ( let entry of data ) { %>
                <tr>
                    <td><input class="form-check-input" type="checkbox" /></td>
                    <td><a href="/product/<%= entry._id %>"><%= entry.name %></a></td>
                    <td><%= entry.brand %></td>
                    <td><%= entry.diameter.toFixed(1) %></td>
                    <td><%= entry.length %></td>
                    <td>$<%= entry.seller.totalPrice %></td>
                </tr>
                <% } %>
            </tbody>
            <tfoot></tfoot>
        </table>

        <div aria-label="Page Navigation">
            <ul class="pagination justify-content-center">
                <li class="page-item">
                    <a class="page-link" href="#" aria-label="Previous">
                        <span aria-hidden="true">&laquo;</span>
                    </a>
                </li>
                <li class="page-item"><a class="page-link" href="#">1</a></li>
                <li class="page-item"><a class="page-link" href="#">2</a></li>
                <li class="page-item"><a class="page-link" href="#">3</a></li>
                <li class="page-item">
                    <a class="page-link" href="#" aria-label="Next">
                        <span aria-hidden="true">&raquo;</span>
                    </a>
                </li>
            </ul>
        </div>
    </div>
</main>